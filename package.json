{
  "name": "LoginApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:detox": "detox test --configuration ios.sim.debug",
    "test:detox:headed": "detox test --configuration ios.sim.debug.headed --headless false",
    "test:detox:build": "detox build --configuration ios.sim.debug",
    "test:detox:clean": "detox clean",
    "test:server": "node e2e/server.js",
    "test:record": "node e2e/record-video.js",
    "test:screenshots": "node e2e/record-video.js --screenshots-only",
    "test:view-screenshots": "node e2e/view-screenshots.js"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.4.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.17.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "puppeteer": "^24.18.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
